{
    "$schema": "https://turbo.build/schema.json",
    "pipeline": {
        "build": {
            "outputs": ["dist/", ".next/", "public/dist/"],
            "dependsOn": ["^build"],
            "env": ["NEXT_PUBLIC_API_HOST"]
        },
        "test": {
            "outputs": ["coverage/"],
            "dependsOn": []
        },
        "lint": {
            "dependsOn": ["^build"],
            "outputs": []
        },
        "dev": {
            "cache": false
        },
        "develop": {
            "cache": false
        },
        "clean": {
            "cache": false
        },
        "production": {
            "cache": false
        },
        "dev:test": {
            "dependsOn": ["^build"]
        },
        "e2e:test": {
            "cache": false
        }
    }
}
